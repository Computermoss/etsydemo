<!-- Any code added here applies to the entire application -->

<!DOCTYPE html>
<html>
<head>
  <title>EtsyDemo</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<%= render 'layouts/header' %>
<div class="container">
  <!--Anything above the yield will show at the top of the page -->
  <%= yield %>
  <!--Anything below the yeild will show on the bottom of the page-->
<%= render 'layouts/footer' %>
</div>

</body>
</html>
